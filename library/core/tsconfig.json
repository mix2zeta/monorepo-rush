{
    "compilerOptions": {
      "moduleResolution": "node",
      "outDir": "dist",
      "allowJs": true,
      "target": "ES6",
      "module": "CommonJS",
      "esModuleInterop": true,
      "strictNullChecks": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "sourceMap": true,
    },
    "include": [
      "./src/**/*",
    ],
    "exclude": ["./node_modules/**/*"],
  }